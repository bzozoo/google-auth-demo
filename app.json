{
  "expo": {
    "owner":"bzozoo",
    "name": "Google Auth Demo",
    "slug": "googleauthdemo",
    "description":"Expo-Auth-Session teszt APP",
    "scheme":"com.bzozoo.googleauthdemo.auth",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bzozoo.googleauthdemo",
      "buildNumber": "3"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "intentFilters":[
        {
          "action": "VIEW",
          "category": ["BROWSABLE", "DEFAULT"],
          "data": {
            "scheme": "com.bzozoo.googleauthdemo"
          }
        }
      ],
      "package": "com.bzozoo.googleauthdemo"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
